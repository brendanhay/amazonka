    Amazonka.TNB.Types.GetSolVnfcResourceInfo
    Amazonka.TNB.Types.GetSolVnfcResourceInfoMetadata
    Amazonka.TNB.Types.GetSolVnfInfo
    Amazonka.TNB.Types.LcmOperationInfo
    Amazonka.TNB.Types.LcmOperationType
    Amazonka.TNB.Types.ListSolFunctionInstanceInfo
    Amazonka.TNB.Types.ListSolFunctionInstanceMetadata
    Amazonka.TNB.Types.ListSolFunctionPackageInfo
    Amazonka.TNB.Types.ListSolFunctionPackageMetadata
    Amazonka.TNB.Types.ListSolNetworkInstanceInfo
    Amazonka.TNB.Types.ListSolNetworkInstanceMetadata
    Amazonka.TNB.Types.ListSolNetworkOperationsInfo
    Amazonka.TNB.Types.ListSolNetworkOperationsMetadata
