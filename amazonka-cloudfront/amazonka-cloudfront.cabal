    Test.AWS.CloudFront
    Test.AWS.CloudFront.Internal
    Test.AWS.Gen.CloudFront

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-cloudfront
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
    , unordered-containers
