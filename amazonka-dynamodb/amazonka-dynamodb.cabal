    Test.AWS.DynamoDB
    Test.AWS.DynamoDB.Internal
    Test.AWS.Gen.DynamoDB

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-dynamodb
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
