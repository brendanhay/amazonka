    Test.AWS.Gen.S3
    Test.AWS.S3
    Test.AWS.S3.Internal

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-s3
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
