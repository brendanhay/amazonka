    Test.Amazonka.Chime
    Test.Amazonka.Chime.Internal
    Test.Amazonka.Gen.Chime

  build-depends:
    , amazonka-chime
    , amazonka-core         >=2.0 && <2.1
    , amazonka-test         >=2.0 && <2.1
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
    , unordered-containers
