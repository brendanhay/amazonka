  -- hand outside these namespaces will not correctly be added to the
  -- distribution package.
  other-modules:
    Test.AWS.Gen.RDS
    Test.AWS.RDS
    Test.AWS.RDS.Internal

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-rds
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
    , unordered-containers
