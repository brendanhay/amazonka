name:                  amazonka-eventbridge
version:               1.6.1
synopsis:              Amazon EventBridge SDK.
homepage:              https://github.com/brendanhay/amazonka
bug-reports:           https://github.com/brendanhay/amazonka/issues
license:               MPL-2.0
license-file:          LICENSE
author:                Brendan Hay
maintainer:            Brendan Hay <brendan.g.hay+amazonka@gmail.com>
copyright:             Copyright (c) 2013-2018 Brendan Hay
category:              Network, AWS, Cloud, Distributed Computing
build-type:            Simple
cabal-version:         >= 1.10
extra-source-files:    README.md fixture/*.yaml fixture/*.proto src/.gitkeep
description:
    The types from this library are intended to be used with
    <http://hackage.haskell.org/package/amazonka amazonka>, which provides
    mechanisms for specifying AuthN/AuthZ information, sending requests,
    and receiving responses.
    .
    Lenses are used for constructing and manipulating types,
    due to the depth of nesting of AWS types and transparency regarding
    de/serialisation into more palatable Haskell values.
    The provided lenses should be compatible with any of the major lens libraries
    such as <http://hackage.haskell.org/package/lens lens> or
    <http://hackage.haskell.org/package/lens-family-core lens-family-core>.
    .
    See "Network.AWS.EventBridge" or <https://aws.amazon.com/documentation/ the AWS documentation>
    to get started.

source-repository head
    type:              git
    location:          git://github.com/brendanhay/amazonka.git
    subdir:            amazonka-eventbridge

library
    default-language:  Haskell2010
    hs-source-dirs:    src gen

    ghc-options:
        -Wall
        -fwarn-incomplete-uni-patterns
        -fwarn-incomplete-record-updates
        -funbox-strict-fields

    exposed-modules:
          Network.AWS.EventBridge
        , Network.AWS.EventBridge.ActivateEventSource
        , Network.AWS.EventBridge.CreateEventBus
        , Network.AWS.EventBridge.CreatePartnerEventSource
        , Network.AWS.EventBridge.DeactivateEventSource
        , Network.AWS.EventBridge.DeleteEventBus
        , Network.AWS.EventBridge.DeletePartnerEventSource
        , Network.AWS.EventBridge.DeleteRule
        , Network.AWS.EventBridge.DescribeEventBus
        , Network.AWS.EventBridge.DescribeEventSource
        , Network.AWS.EventBridge.DescribePartnerEventSource
        , Network.AWS.EventBridge.DescribeRule
        , Network.AWS.EventBridge.DisableRule
        , Network.AWS.EventBridge.EnableRule
        , Network.AWS.EventBridge.ListEventBuses
        , Network.AWS.EventBridge.ListEventSources
        , Network.AWS.EventBridge.ListPartnerEventSourceAccounts
        , Network.AWS.EventBridge.ListPartnerEventSources
        , Network.AWS.EventBridge.ListRuleNamesByTarget
        , Network.AWS.EventBridge.ListRules
        , Network.AWS.EventBridge.ListTagsForResource
        , Network.AWS.EventBridge.ListTargetsByRule
        , Network.AWS.EventBridge.PutEvents
        , Network.AWS.EventBridge.PutPartnerEvents
        , Network.AWS.EventBridge.PutPermission
        , Network.AWS.EventBridge.PutRule
        , Network.AWS.EventBridge.PutTargets
        , Network.AWS.EventBridge.RemovePermission
        , Network.AWS.EventBridge.RemoveTargets
        , Network.AWS.EventBridge.TagResource
        , Network.AWS.EventBridge.TestEventPattern
        , Network.AWS.EventBridge.Types
        , Network.AWS.EventBridge.UntagResource
        , Network.AWS.EventBridge.Waiters

    other-modules:
          Network.AWS.EventBridge.Types.Product
        , Network.AWS.EventBridge.Types.Sum

    build-depends:
          amazonka-core == 1.6.1.*
        , base          >= 4.7     && < 5

test-suite amazonka-eventbridge-test
    type:              exitcode-stdio-1.0
    default-language:  Haskell2010
    hs-source-dirs:    test
    main-is:           Main.hs

    ghc-options:       -Wall -threaded

    -- This section is encoded by the template and any modules added by
    -- hand outside these namespaces will not correctly be added to the
    -- distribution package.
    other-modules:
          Test.AWS.EventBridge
        , Test.AWS.Gen.EventBridge
        , Test.AWS.EventBridge.Internal

    build-depends:
          amazonka-core == 1.6.1.*
        , amazonka-test == 1.6.1.*
        , amazonka-eventbridge
        , base
        , bytestring
        , tasty
        , tasty-hunit
        , text
        , time
        , unordered-containers
