    Test.Amazonka.AlexaBusiness
    Test.Amazonka.AlexaBusiness.Internal
    Test.Amazonka.Gen.AlexaBusiness

  build-depends:
    , amazonka-alexa-business
    , amazonka-core            >=2.0 && <2.1
    , amazonka-test            >=2.0 && <2.1
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
    , unordered-containers
