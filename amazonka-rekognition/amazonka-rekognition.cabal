    Test.AWS.Gen.Rekognition
    Test.AWS.Rekognition
    Test.AWS.Rekognition.Internal

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-rekognition
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
