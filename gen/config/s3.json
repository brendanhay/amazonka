{
    "libraryName": "amazonka-s3",
    "extraDependencies": [
        "text >= 1.1",
        "lens >= 4.4"
    ],
    "operationPlugins": {
        "DeleteObjects": ["contentMD5Header"],
        "PutBucketCORS": ["contentMD5Header"],
        "PutBucketLifecycle": ["contentMD5Header"],
        "PutBucketPolicy": ["contentMD5Header"],
        "PutBucketTagging": ["contentMD5Header"],
        "PutObject": ["expectHeader"]
    },
    "typeModules": [
        "Network.AWS.S3.Internal"
    ],
    "typeOverrides": {
        "GetBucketPolicyOutput": {
            "requiredFields": [
                "Policy"
            ]
        },
        "Error": {
            "renamedTo": "S3ServiceError"
        },
        "Bucket": {
            "requiredFields": [
                "CreationDate",
                "Name"
            ]
        },
        "Object": {
            "requiredFields": [
                "ETag",
                "Size",
                "Key",
                "StorageClass",
                "LastModified"
            ]
        },
        "CompletedPart": {
            "requiredFields": [
                "PartNumber",
                "ETag"
            ]
        },
        "BucketLocationConstraint": {
            "replacedBy": {
                "name": "LocationConstraint",
                "deriving": []
            }
        },
        "BucketName": {
            "replacedBy": {
                "name": "BucketName",
                "deriving": []
            }
        },
        "ObjectKey": {
            "replacedBy": {
                "name": "ObjectKey",
                "deriving": []
            }
        },
        "ObjectVersionId": {
            "replacedBy": {
                "name": "ObjectVersionId",
                "deriving": []
            }
        },
        "ETag": {
            "replacedBy": {
                "name": "ETag",
                "deriving": []
            }
        },
        "Delimiter": {
            "replacedBy": {
                "name": "Delimiter",
                "deriving": []
            }
        },
        "NoncurrentVersionExpiration": {
            "requiredFields": [
                "NoncurrentDays"
            ]
        },
        "NoncurrentVersionTransition": {
            "requiredFields": [
                "NoncurrentDays",
                "StorageClass"
            ]
        },
        "BucketCannedACL": {
            "enumPrefix": "B"
        },
        "ObjectCannedACL": {
            "enumPrefix": "O"
        },
        "InventoryOptionalField": {
            "enumPrefix": "Field"
        }
    }
}
