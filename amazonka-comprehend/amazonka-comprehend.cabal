    Test.AWS.Comprehend
    Test.AWS.Comprehend.Internal
    Test.AWS.Gen.Comprehend

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-comprehend
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
