  -- hand outside these namespaces will not correctly be added to the
  -- distribution package.
  other-modules:
    Test.AWS.Gen.Route53
    Test.AWS.Route53
    Test.AWS.Route53.Internal

  build-depends:
    , amazonka              >=1.6.1 && <1.6.2
    , amazonka-route53
    , amazonka-test         >=1.6.1 && <1.6.2
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
