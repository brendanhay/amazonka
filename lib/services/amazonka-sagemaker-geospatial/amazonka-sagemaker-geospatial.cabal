    Test.Amazonka.Gen.SageMakerGeoSpatial
    Test.Amazonka.SageMakerGeoSpatial
    Test.Amazonka.SageMakerGeoSpatial.Internal

  build-depends:
    , amazonka-core                  >=2.0 && <2.1
    , amazonka-sagemaker-geospatial
    , amazonka-test                  >=2.0 && <2.1
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
