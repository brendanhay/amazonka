  -- hand outside these namespaces will not correctly be added to the
  -- distribution package.
  other-modules:
    Test.Amazonka.Gen.RDS
    Test.Amazonka.RDS
    Test.Amazonka.RDS.Internal

  build-depends:
    , amazonka-core         >=2.0 && <2.1
    , amazonka-rds
    , amazonka-test         >=2.0 && <2.1
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
    , unordered-containers
