  -- hand outside these namespaces will not correctly be added to the
  -- distribution package.
  other-modules:
    Test.Amazonka.DMS
    Test.Amazonka.DMS.Internal
    Test.Amazonka.Gen.DMS

  build-depends:
    , amazonka-core         >=2.0 && <2.1
    , amazonka-dms
    , amazonka-test         >=2.0 && <2.1
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
    , time
    , unordered-containers
