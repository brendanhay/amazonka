    Test.Amazonka.Gen.MacieV2
    Test.Amazonka.MacieV2
    Test.Amazonka.MacieV2.Internal

  build-depends:
    , amazonka-core         >=2.0 && <2.1
    , amazonka-maciev2
    , amazonka-test         >=2.0 && <2.1
    , base
    , bytestring
    , case-insensitive
    , tasty
    , tasty-hunit
    , text
